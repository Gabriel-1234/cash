{
  "deploymentType": "docker",
  "name": "GCash Frontend",
  "description": "Money transfer application frontend UI",
  "ports": [
    {
      "port": 5173,
      "protocol": "http"
    }
  ],
  "dockerfile": "Dockerfile.frontend",
  "rootDirectory": ".",
  "environment": {
    "VITE_API_URL": "http://backend:5002/api"
  },
  "healthCheck": {
    "enabled": true,
    "path": "/",
    "protocol": "http",
    "interval": 10,
    "timeout": 5,
    "retries": 5,
    "startPeriod": 20
  }
}
